package com.revature;

public class Driver {

	public static void main(String... args) {
		//System.out.println(args[0]);
		int[] myInts= {8,9,2,3,7};
		display("Dave", myInts);
		display("Chris", 1,2,3,4,5);
		objects("It","was","the","best","of","Times");
	}
	//your var-args must be last in the arg list
	public static void display(String str, int... arr) {
		System.out.println(str);
		
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]);
		}
		
	}
	public static void objects(Object...objects ) {
		for(int i=0;i<objects.length;i++) {
			System.out.println(objects[i]);
		}
	}
	
	
}
